{
  "manifest_version": 3,
  "name": "LeetCode Fetcher",
  "version": "2.0",
  "permissions": ["scripting", "activeTab","tabs"],
  "host_permissions": ["http://127.0.0.1:5000/*","https://*/*","http://*/*"],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html"
  }
}
